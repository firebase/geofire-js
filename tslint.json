{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest"
    ],
    "linterOptions": {
        "exclude": [
            "node_modules/**",
            "dist/**"
        ]
    },
    "jsRules": {},
    "rules": {
      "no-bitwise": false,
      "no-consecutive-blank-lines": false,
      "no-console": false,
      "quotemark": [
        true,
        "single"
      ],
      "variable-name": [
        true,
        "ban-keywords"
      ],
      "array-type": [
        true,
        "array"
      ],
      "interface-name": false,
      "max-line-length": [true, 150]
    },
    "interface-name": [true, "never-prefix"],
    "rulesDirectory": []
}
